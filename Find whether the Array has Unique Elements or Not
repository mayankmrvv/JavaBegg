public class JavaApplication99 {
public static boolean unique(int a[]){
    for(int i=0; i<=a.length-1; i++){
        for(int j=i+1; j<=a.length-1; j++){
            if(a[i]==a[j]){
           return false;
        }
    }
   
    }
    return true;
}

    public static void main(String[] args) {
        int arr[]={1,2,3,4,5,6};
        System.out.println(unique(arr));
    }
    
}
