import java.util.Scanner;

public class JavaApplication65 {

  public static int power2(int a, int b){
      int output=a;
      for(int i=1; i<b; i++){
      output = a*output;
     
      }
      return output;
  }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the your number");
        int c = sc.nextInt();
        System.out.println("Enter the desired power of "+c);
        int d = sc.nextInt();
       System.out.println("The Value of "+c+" raised to the power"+" "+ d+" is "+power2(c,d));
    }
    
}
